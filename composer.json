{
    "name": "freshcells/state-inspector",
    "type": "library",
    "description": "inspects the state of a given object",
    "keywords": [
        "state-inspector"
    ],
    "homepage": "https://github.com/freshcells/state-inspector",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivo Bathke",
            "email": "ivo.bathke@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.4|^8.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.0",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Freshcells\\StateInspector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Freshcells\\StateInspector\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
